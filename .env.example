# Appwrite Configuration
APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
APPWRITE_PROJECT_ID=your_project_id_here
APPWRITE_DATABASE_ID=astro_gpt_db

# Collection IDs
COLLECTION_USERS=users
COLLECTION_ASTROLOGERS=astrologers
COLLECTION_CHAT_SESSIONS=chat_sessions
COLLECTION_MESSAGES=messages
COLLECTION_HOROSCOPES=horoscopes
COLLECTION_DAILY_CONTENT=daily_content
COLLECTION_REVIEWS=reviews
COLLECTION_FAVORITES=favorites
COLLECTION_FAQS=faqs
COLLECTION_SUBSCRIPTIONS=subscriptions

# Storage Bucket IDs
BUCKET_PROFILE_IMAGES=profile_images
BUCKET_ASTROLOGER_IMAGES=astrologer_images
BUCKET_DEITY_IMAGES=deity_images

# AdMob (Replace with your IDs in production)
ADMOB_ANDROID_APP_ID=your_android_app_id
ADMOB_IOS_APP_ID=your_ios_app_id
ADMOB_BANNER_ID=your_banner_id
ADMOB_INTERSTITIAL_ID=your_interstitial_id
ADMOB_REWARDED_ID=your_rewarded_id

# App Configuration
APP_NAME=Astro GPT
APP_ENV=development
DEBUG=true

# Service Configuration
# Set to 'true' to use mock services instead of Appwrite
USE_MOCKS=false
